{
  "name": "ngxp-groceries-app",
  "version": "1.0.0",
  "description": "A (really) cross platform groceries application.",
  "author": "Mathieu Brunot <mathieu.brunot@monogramm.io> (https://github/monogramm)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Monogramm/ngxp-groceries-app.git"
  },
  "bugs": {
    "url": "https://github.com/Monogramm/ngxp-groceries-app/issues"
  },
  "homepage": "https://github.com/Monogramm/ngxp-groceries-app/groceries",
  "keywords": [
    "nativescript",
    "angular4",
    "ng4",
    "angular"
  ],
  "scripts": {
    "ngxp-install": "npm install && cd web && npm install && cd ../nativescript && tns install && node ../tools/install.js && cd .. && typings install",
    "build": "cd web && ng build",
    "build.prod": "cd web && ng build --prod",
    "build.aot": "cd web && ng build --aot",
    "build.aot.prod": "cd web && ng build --aot --prod",
    "start": "cd web && ng serve",
    "start.prod": "cd web && ng serve --prod",
    "start.aot": "cd web && ng serve --aot",
    "start.aot.prod": "cd web && ng serve --aot --prod",
    "start.android": "cd nativescript && tns run android",
    "start.ios": "cd nativescript && tns run ios",
    "clean.android": "cd nativescript && tns platform clean android",
    "clean.ios": "cd nativescript && tns platform clean ios",
    "restart.android": "cd nativescript && tns platform clean android && tns run android",
    "restart.ios": "cd nativescript && tns platform clean ios && tns run ios",
    "test": "cd web && ng test",
    "test-cc": "npm test -- -cc",
    "test.android": "cd nativescript && tns test android --watch",
    "test.ios": "cd nativescript && tns test ios --watch"
  },
  "devDependencies": {
    "@types/jasmine": "~2.2.30",
    "codelyzer": "~3.1.1",
    "tslint": "^5.4.3",
    "typings": "~2.1.1"
  },
  "dependencies": {
    "@angular/animations": "~4.2.3",
    "@angular/common": "~4.2.3",
    "@angular/compiler": "~4.2.3",
    "@angular/compiler-cli": "~4.2.3",
    "@angular/core": "~4.2.3",
    "@angular/forms": "~4.2.3",
    "@angular/http": "~4.2.3",
    "@angular/platform-browser": "~4.2.3",
    "@angular/platform-browser-dynamic": "~4.2.3",
    "@angular/platform-server": "~4.2.3",
    "@angular/router": "~4.2.3",
    "core-js": "~2.4.1",
    "email-validator": "^1.0.7",
    "rxjs": "^5.4.0",
    "ts-helpers": "~1.1.1",
    "validate-password": "^1.0.4",
    "zone.js": "~0.8.17"
  }
}